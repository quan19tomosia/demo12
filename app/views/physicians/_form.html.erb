<%= form_with(model: physician) do |form| %>
  <% if physician.errors.any? %>
    <div style="color: red">
      <h2>
        <%= pluralize(physician.errors.count, "error" ) %> prohibited this physician from being saved:
      </h2>

      <ul>
        <% physician.errors.each do |error| %>
          <li>
            <%= error.full_message %>
          </li>
          <% end %>
      </ul>
    </div>
  <% end %>
  <div class="row">
    <div class="col-xl">
      <div class="card mb-4">
        <div class="card-header d-flex justify-content-between align-items-center">
          <h5 class="mb-0">Basic with Icons</h5>
          <small class="text-muted float-end">Merged input group</small>
        </div>
        <div class="card-body">
          <div class="mb-3">
            <%= form.label :full_name, class: "form-label" %>
            <div class="input-group input-group-merge">
              <span id="basic-icon-default-fullname2" class="input-group-text"><i class="bi bi-person"></i></span>
              <%= form.text_field :name, class: "form-control", placeholder: "John Doe" %>
            </div>
          </div>
          <div class="mb-3">
            <%= form.label :email, class: "form-label" %>
            <div class="input-group input-group-merge">
              <span class="input-group-text"><i class="bi bi-envelope"></i></span>
              <%= form.email_field :email, class: "form-control", placeholder: "john.doe" %>
              <span id="basic-icon-default-email2" class="input-group-text">@example.com</span>
            </div>
            <div class="form-text">You can use letters, numbers & periods</div>
          </div>
          <div class="mb-3">
            <%= form.label :phone, class: "form-label" %>
            <div class="input-group input-group-merge">
              <span id="basic-icon-default-phone2" class="input-group-text"><i class="bi bi-telephone"></i></span>
              <%= form.text_field :phone, class: "form-control phone-mask", placeholder: "658 799 8941" %>
            </div>
          </div>
        </div>
      </div>
      <div class="card mb-4">
        <div class="d-flex align-items-start align-items-sm-center gap-4">
          <img src="data:image/png;base64," alt="user-avatar" class="d-block rounded" height="100" width="100" id="uploadedAvatar" />
          <div class="button-wrapper">
            <label for="upload" class="btn btn-primary me-2 mb-4" tabindex="0">
              <span class="d-none d-sm-block">Upload new photo</span>
              <i class="bx bx-upload d-block d-sm-none"></i>
              <%# <%= form.file_field :picture %>
              <input type="file" id="upload" class="account-file-input" hidden accept="image/png, image/jpeg" />
            </label>
            <button type="button" class="btn btn-outline-secondary account-image-reset mb-4">
              <i class="bx bx-reset d-block d-sm-none"></i>
              <span class="d-none d-sm-block">Reset</span>
            </button>

            <p class="text-muted mb-0">Allowed JPG, GIF or PNG. Max size of 800K</p>
          </div>
        </div>
      </div>
    </div>
    <div class="col-xl">
      <div class="card mb-4">
        <div class="card-header d-flex justify-content-between align-items-center">
          <h5 class="mb-0">Physician fill</h5>
          <small class="text-muted float-end">Merged input group</small>
        </div>
      <div class="card-body">
        <div class="mb-3">
          <%= form.label :degree, class: "form-label" %>
          <div class="input-group input-group-merge">
            <span class="input-group-text"><i class="bi bi-person-badge"></i></span>
            <%= form.text_field :degree, class: "form-control" %>
          </div>
        </div>
        <div class="mb-3">
          <%= form.label :education, class: "form-label" %>
          <div class="input-group input-group-merge">
            <span class="input-group-text"><i class="bi bi-mortarboard"></i></span>
            <%= form.email_field :education, class: "form-control" %>
          </div>
        </div>
        <div class="mb-3">
          <%= form.label :experience, class: "form-label" %>
          <div class="input-group input-group-merge">
            <span class="input-group-text"><i class="bi bi-person-video3"></i></span>
            <%= form.text_area :experience, class: "form-control" %>
          </div>
        </div>
        <div class="mb-3">
          <%= form.label :about, class: "form-label" %>
          <div class="input-group input-group-merge">
            <span class="input-group-text"><i class="bi bi-chat-square-quote"></i></span>
            <%= form.text_area :about, class: "form-control" %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div>
    <%= form.submit nil, class: "btn btn-primary" %>
  </div>
<% end %>